<div class="list-group">
   <h2>Shoes: </h2>
   <ul class="list-group" id="shoe">
   {{#each shoes}}
     <li class="list-group-item list-group-item-dark">
       <h4>{{this.model}}</h4>
       Brand: {{this.brand}} 
       <br>Color: {{this.color}}
       <br>ID: {{this.id}}
     </li>
   {{/each}}
   </ul>
</div>
